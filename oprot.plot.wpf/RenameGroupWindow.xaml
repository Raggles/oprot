<Window x:Class="oprot.plot.wpf.RenameGroupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:oprot.plot.wpf"
        mc:Ignorable="d"
        Title="Rename Group..." Height="100" Width="200" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel>
        <TextBox x:Name="txtName" />
        <Button Content="Rename" Click="Button_Click" />
        </StackPanel>
    </Grid>
</Window>
